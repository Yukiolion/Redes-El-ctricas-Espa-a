Balance electrico 2019/codigo: 
import pandas as pd

# Cargar el archivo
df = pd.read_csv("ruta/del/archivo.csv")

# Convertir columna 'fecha' a datetime si existe
if 'fecha' in df.columns:
    df['fecha'] = pd.to_datetime(df['fecha'], errors='coerce')
    df = df.dropna(subset=['fecha'])

# Eliminar duplicados
df = df.drop_duplicates()

# Limpiar espacios en blanco en columnas de texto
for col in df.select_dtypes(include='object').columns:
    df[col] = df[col].str.strip()

# Eliminar filas con cualquier valor nulo
df = df.dropna()

# Guardar el archivo limpio
df.to_csv("ruta/del/archivo_limpio.csv", index=False)

Balance electrico 2020/codigo: 
import pandas as pd

# Cargar el archivo
df = pd.read_csv("ruta/del/archivo.csv")

# Convertir la columna 'fecha' a tipo datetime
if 'fecha' in df.columns:
    df['fecha'] = pd.to_datetime(df['fecha'], errors='coerce')
    df = df.dropna(subset=['fecha'])

# Eliminar duplicados
df = df.drop_duplicates()

# Limpiar espacios en columnas de texto
for col in df.select_dtypes(include='object').columns:
    df[col] = df[col].str.strip()

# Eliminar filas con valores nulos
df = df.dropna()

# Guardar el archivo limpio
df.to_csv("ruta/del/archivo_limpio.csv", index=False)

Balance electrico 2021/codigo: 

import pandas as pd

# Cargar el archivo CSV
df = pd.read_csv("ruta/del/archivo.csv")

# Convertir columna 'fecha' a datetime si existe
if 'fecha' in df.columns:
    df['fecha'] = pd.to_datetime(df['fecha'], errors='coerce')
    df = df.dropna(subset=['fecha'])

# Eliminar duplicados
df = df.drop_duplicates()

# Limpiar texto: eliminar espacios al inicio y final
for col in df.select_dtypes(include='object').columns:
    df[col] = df[col].str.strip()

# Eliminar filas con valores faltantes
df = df.dropna()

# Guardar el archivo limpio
df.to_csv("ruta/del/archivo_limpio.csv", index=False)
Balance electrico 2022/codigo: 

import pandas as pd

# Cargar archivo CSV
df = pd.read_csv("ruta/del/archivo.csv")

# Convertir columna 'fecha' a tipo datetime si existe
if 'fecha' in df.columns:
    df['fecha'] = pd.to_datetime(df['fecha'], errors='coerce')
    df = df.dropna(subset=['fecha'])

# Eliminar duplicados
df = df.drop_duplicates()

# Limpiar texto (espacios en blanco) en columnas tipo string
for col in df.select_dtypes(include='object').columns:
    df[col] = df[col].str.strip()

# Eliminar filas con valores nulos
df = df.dropna()

# Guardar archivo limpio
df.to_csv("ruta/del/archivo_limpio.csv", index=False)
Balance electrico 2023/codigo:

import pandas as pd

# Cargar el archivo CSV
df = pd.read_csv("ruta/del/archivo.csv")

# Convertir la columna 'fecha' a tipo datetime si existe
if 'fecha' in df.columns:
    df['fecha'] = pd.to_datetime(df['fecha'], errors='coerce')
    df = df.dropna(subset=['fecha'])

# Eliminar duplicados
df = df.drop_duplicates()

# Limpiar texto: eliminar espacios en columnas tipo string
for col in df.select_dtypes(include='object').columns:
    df[col] = df[col].str.strip()

# Eliminar filas con valores faltantes
df = df.dropna()

# Guardar el archivo limpio
df.to_csv("ruta/del/archivo_limpio.csv", index=False)
