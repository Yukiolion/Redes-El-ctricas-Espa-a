General anual 2019/codigo: 

import pandas as pd

# Cargar el archivo
df = pd.read_csv("ruta/del/archivo.csv")

# Conversión de fechas
df['fecha'] = pd.to_datetime(df['fecha'], errors='coerce')

# Eliminar filas con fechas inválidas o valores nulos
df = df.dropna(subset=['fecha'])

# Eliminar duplicados
df = df.drop_duplicates()

# Limpiar espacios en columnas de texto
for col in ['indicator', 'region']:
    if col in df.columns:
        df[col] = df[col].astype(str).str.strip()

# Eliminar cualquier otra fila con valores nulos
df = df.dropna()

# Guardar archivo limpio
df.to_csv("ruta/del/archivo_limpio.csv", index=False)

General anual 2020/codigo: 
import pandas as pd

# Cargar archivo
df = pd.read_csv("ruta/del/archivo.csv")

# Convertir columna 'fecha' a tipo datetime
df['fecha'] = pd.to_datetime(df['fecha'], errors='coerce')

# Eliminar filas con fechas inválidas
df = df.dropna(subset=['fecha'])

# Eliminar duplicados
df = df.drop_duplicates()

# Eliminar espacios en blanco en columnas de texto
for col in ['indicator', 'region']:
    if col in df.columns:
        df[col] = df[col].astype(str).str.strip()

# Eliminar cualquier fila con valores nulos
df = df.dropna()

# Guardar archivo limpio
df.to_csv("ruta/del/archivo_limpio.csv", index=False)

General anual 2021/codigo: 

import pandas as pd

# Cargar archivo CSV
df = pd.read_csv("ruta/del/archivo.csv")

# Convertir 'fecha' a tipo datetime
df['fecha'] = pd.to_datetime(df['fecha'], errors='coerce')

# Eliminar filas con fechas no válidas
df = df.dropna(subset=['fecha'])

# Eliminar duplicados
df = df.drop_duplicates()

# Limpiar espacios en columnas de texto
for col in ['indicator', 'region']:
    if col in df.columns:
        df[col] = df[col].astype(str).str.strip()

# Eliminar otras filas con valores nulos
df = df.dropna()

# Guardar archivo limpio
df.to_csv("ruta/del/archivo_limpio.csv", index=False)

General anual 2022/codigo: 
import pandas as pd

# Cargar el archivo CSV
df = pd.read_csv("ruta/del/archivo.csv")

# Convertir a tipo datetime
df['fecha'] = pd.to_datetime(df['fecha'], errors='coerce')

# Eliminar fechas inválidas
df = df.dropna(subset=['fecha'])

# Eliminar duplicados
df = df.drop_duplicates()

# Limpiar espacios en columnas de texto
for col in ['indicator', 'region']:
    if col in df.columns:
        df[col] = df[col].astype(str).str.strip()

# Eliminar filas con otros valores nulos
df = df.dropna()

# Guardar archivo limpio
df.to_csv("ruta/del/archivo_limpio.csv", index=False)

General anual 2023/codigo: 

import pandas as pd

# Cargar el archivo
df = pd.read_csv("ruta/del/archivo.csv")

# Convertir la columna 'fecha' a tipo datetime
df['fecha'] = pd.to_datetime(df['fecha'], errors='coerce')

# Eliminar filas con fechas inválidas
df = df.dropna(subset=['fecha'])

# Eliminar duplicados
df = df.drop_duplicates()

# Limpiar espacios en blanco en columnas de texto
for col in ['indicator', 'region']:
    if col in df.columns:
        df[col] = df[col].astype(str).str.strip()

# Eliminar cualquier otra fila con valores nulos
df = df.dropna()

# Guardar el archivo limpio
df.to_csv("ruta/del/archivo_limpio.csv", index=False)

